<div id="iniciosesion" class="col-md-4 mt-4 mt-sm-0">
    <h2>Iniciar Sesión</h2>
    <form class="row g-3" action="/iniciosesion" method="pst">
    <div class="col-md-12">
        <label for="email" class="form-label">Email</label>
        <input type="email" class="form-control" id="email" name="email" placeholder="email@email.com">
    </div>
    <div class="col-md-12">
        <label for="contraseña" class="form-label">Contraseña</label>
        <input type="contraseña" class="form-control" id="contraseña" name="contraseña" placeholder="***">
    </div>
    <div class="col-md-12">
        <button class="btn btn-outline-success" type="submit" class="btn btn primary">Iniciar Sesión</button>
    </div>
    </form>
</div>

<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>